package com.ssm.beans;

import java.io.Serializable;
import java.util.List;

public class DeviceInfoBean implements Serializable {
	
	private String id;
	private String deviceName;
	private String enterpriseId;
	private String enterpriseName;
	private Integer probeNumber;
	private Integer controllerNumber;
	private String serialNumber;
	private String typeId;
	private String verificationCode;
	private String createTime;
	private String updateTime;
	private String comments;
	private List<ProbeBean> probeList;
	private List<PowerControllerBean> powerControllerList ;
	
	public DeviceInfoBean() {
		super(); 
	}
	
	public DeviceInfoBean(String id, String deviceName, String enterpriseId,
			String enterpriseName, Integer probeNumber, Integer controllerNumber,
			String serialNumber, String typeId, String verificationTode,
			String comments) {
		super();
		this.id = id;
		this.deviceName = deviceName;
		this.enterpriseId = enterpriseId;
		this.enterpriseName = enterpriseName;
		this.probeNumber = probeNumber;
		this.controllerNumber = controllerNumber;
		this.serialNumber = serialNumber;
		this.typeId = typeId;
		this.verificationCode = verificationTode;
		this.comments = comments;
	}

	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getDeviceName() {
		return deviceName;
	}
	public void setDeviceName(String deviceName) {
		this.deviceName = deviceName;
	}
	public String getEnterpriseId() {
		return enterpriseId;
	}
	public void setEnterpriseId(String enterpriseId) {
		this.enterpriseId = enterpriseId;
	}
	public String getEnterpriseName() {
		return enterpriseName;
	}
	public void setEnterpriseName(String enterpriseName) {
		this.enterpriseName = enterpriseName;
	}
	public Integer getProbeNumber() {
		return probeNumber;
	}
	public void setProbeNumber(Integer probeNumber) {
		this.probeNumber = probeNumber;
	}
	public Integer getControllerNumber() {
		return controllerNumber;
	}
	public void setControllerNumber(Integer controllerNumber) {
		this.controllerNumber = controllerNumber;
	}
	public String getSerialNumber() {
		return serialNumber;
	}
	public void setSerialNumber(String serialNumber) {
		this.serialNumber = serialNumber;
	}
	public String getTypeId() {
		return typeId;
	}
	public void setTypeId(String typeId) {
		this.typeId = typeId;
	}
	public String getVerificationCode() {
		return verificationCode;
	}
	public void setVerificationCode(String verificationCode) {
		this.verificationCode = verificationCode;
	}
	public String getCreateTime() {
		return createTime;
	}
	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}
	public String getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}
	public String getComments() {
		return comments;
	}
	public void setComments(String comments) {
		this.comments = comments;
	}

	public List<ProbeBean> getProbeList() {
		return probeList;
	}

	public void setProbeList(List<ProbeBean> probeList) {
		this.probeList = probeList;
	}

	public List<PowerControllerBean> getPowerControllerList() {
		return powerControllerList;
	}

	public void setPowerControllerList(List<PowerControllerBean> powerControllerList) {
		this.powerControllerList = powerControllerList;
	}

	@Override
	public String toString() {
		return "DeviceInfoBean [id=" + id + ", deviceName=" + deviceName
				+ ", enterpriseId=" + enterpriseId + ", enterpriseName="
				+ enterpriseName + ", probeNumber=" + probeNumber
				+ ", controllerNumber=" + controllerNumber + ", serialNumber="
				+ serialNumber + ", typeId=" + typeId + ", verificationCode="
				+ verificationCode + ", createTime=" + createTime
				+ ", updateTime=" + updateTime + ", powerControllerList=" + powerControllerList
				+ ",\n\r probeList=" + probeList + "]";
	}
	

}

