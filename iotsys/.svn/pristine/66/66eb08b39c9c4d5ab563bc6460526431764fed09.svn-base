package com.ssm.util;

import java.util.HashMap;
import java.util.Map;

public class CodeMap {
	
	public static Map <String,String> codeMap = new HashMap();  
	
	public static void put(String regex , String codeValue){
		codeMap.put(regex, codeValue);
	}
	
	public static String getValue(String codetype,String codekey){
		return codeMap.get(codetype + codekey);
	}
}
