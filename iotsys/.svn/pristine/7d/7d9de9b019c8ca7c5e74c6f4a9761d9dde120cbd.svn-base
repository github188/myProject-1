package com.ssm.beans;

import java.io.Serializable;
import java.util.List;

//智能控制包装类
public class SmartControllerBean implements Serializable{
	
	private String id;//ID
	private String controllerName;//设备名称
	private String deviceSerialNumber;//设备序号
	private int surveyNo;//通道号
	private String powerSwitch;//开关 开是3  关是0 默认关  只能是0 3
	private String status;//状态  默认0
	private String icoName;//控制器图标
	private int sortNo;//排序
	private String createTime;//创建时间
	private String updateTime;//更新时间
	private String comments;//描述
	private List<TimerControllerInfoBean> timerControllerInfoList;  //时间控制
	private List<ParameterControllerInfoBean> parameterControllerInfoList;  //参数控制
	
	
	@Override
	public String toString() {
		return "SmartControllerBean [id=" + id + ", controllerName=" + controllerName + ", deviceSerialNumber="
				+ deviceSerialNumber + ", surveyNo=" + surveyNo + ", powerSwitch=" + powerSwitch + ", status=" + status
				+ ", icoName=" + icoName + ", sortNo=" + sortNo + ", createTime=" + createTime + ", updateTime="
				+ updateTime + ", comments=" + comments + ", timerControllerInfoList=" + timerControllerInfoList
				+ ", parameterControllerInfoList=" + parameterControllerInfoList + "]";
	}
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getControllerName() {
		return controllerName;
	}
	public void setControllerName(String controllerName) {
		this.controllerName = controllerName;
	}
	public String getDeviceSerialNumber() {
		return deviceSerialNumber;
	}
	public void setDeviceSerialNumber(String deviceSerialNumber) {
		this.deviceSerialNumber = deviceSerialNumber;
	}
	public Integer getSurveyNo() {
		return surveyNo;
	}
	public void setSurveyNo(Integer surveyNo) {
		this.surveyNo = surveyNo;
	}
	public String getPowerSwitch() {
		return powerSwitch;
	}
	public void setPowerSwitch(String powerSwitch) {
		this.powerSwitch = powerSwitch;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getIcoName() {
		return icoName;
	}
	public void setIcoName(String icoName) {
		this.icoName = icoName;
	}
	public int getSortNo() {
		return sortNo;
	}
	public void setSortNo(int sortNo) {
		this.sortNo = sortNo;
	}
	public String getCreateTime() {
		return createTime;
	}
	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}
	public String getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}
	public String getComments() {
		return comments;
	}
	public void setComments(String comments) {
		this.comments = comments;
	}
	public List<TimerControllerInfoBean> getTimerControllerInfoList() {
		return timerControllerInfoList;
	}
	public void setTimerControllerInfoList(List<TimerControllerInfoBean> timerControllerInfoList) {
		this.timerControllerInfoList = timerControllerInfoList;
	}
	public List<ParameterControllerInfoBean> getParameterControllerInfoList() {
		return parameterControllerInfoList;
	}
	public void setParameterControllerInfoList(List<ParameterControllerInfoBean> parameterControllerInfoList) {
		this.parameterControllerInfoList = parameterControllerInfoList;
	}

	
	
	
	
	
	
}
